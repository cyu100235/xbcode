# 是否开启调试模式
APP_DEBUG = false

# CPU倍数
WORKER_COUNT = 1

# 是否开启商店与版本更新
PLUGIN_STORE = true

# 设置系统时区
DEFAULT_TIMEZONE = Asia/Shanghai

# 数据库配置
[DATABASE]
TYPE = {TYPE}
HOSTNAME = {HOSTNAME}
DATABASE = {DATABASE}
USERNAME = {USERNAME}
PASSWORD = {PASSWORD}
HOSTPORT = {HOSTPORT}
CHARSET = utf8mb4
PREFIX = {PREFIX}

# 缓存配置
[CACHE]
TYPE = file

# REDIS配置
[REDIS]
HOST = {REDIS_HOST}
PORT = {REDIS_PORT}
PASSWD = {REDIS_PASSWD}
DATABASE = 0
EXPIRE_TIME = 0
PREFIX = {REDIS_PREFIX}
TAG_EXPIRE_TIME = 3600
TAG_PREFIX = xb_tag_